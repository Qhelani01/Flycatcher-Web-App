{
  "version": 2,
  "functions": {
    "api/config.py": {
      "runtime": "python3.9"
    },
    "api/observations.py": {
      "runtime": "python3.9"
    },
    "api/species.py": {
      "runtime": "python3.9"
    }
  },
  "routes": [
    {
      "src": "/api/config",
      "dest": "/api/config.py"
    },
    {
      "src": "/api/observations",
      "dest": "/api/observations.py"
    },
    {
      "src": "/api/species/(.*)",
      "dest": "/api/species.py"
    },
    {
      "src": "/(.*)",
      "dest": "/frontend/$1"
    }
  ]
}
